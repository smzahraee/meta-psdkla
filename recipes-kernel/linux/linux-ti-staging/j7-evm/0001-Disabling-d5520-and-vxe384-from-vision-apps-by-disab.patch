+++ b/arch/arm64/boot/dts/ti/k3-j721e-vision-apps.dtso
@@ -286,3 +286,11 @@
 &ti_csi2rx0 {
 	status = "disabled";
 };
+
+&d5520 {
+       status = "disabled";
+};
+
+&vxe384 {
+       status = "disabled";
+};
