+ * Copyright (C) 2020 Texas Instruments Incorporated - https://www.ti.com/
+ */
+
+/dts-v1/;
+/plugin/;
+
+&mcu_r5fss0_core0 {
+	status = "disabled";
+};
+
+&mcu_r5fss0_core1 {
+	status = "disabled";
+};
+
+&main_r5fss0_core0 {
+	status = "disabled";
+};
+
+&main_r5fss0_core1 {
+	status = "disabled";
+};
\ No newline at end of file
