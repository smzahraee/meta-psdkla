+				   k3-j7200-dra821a.dtbo
 
 dtb-$(CONFIG_ARCH_K3) += k3-am642-evm.dtb
diff --git a/arch/arm64/boot/dts/ti/k3-j7200-dra821a.dtso b/arch/arm64/boot/dts/ti/k3-j7200-dra821a.dtso
new file mode 100644
